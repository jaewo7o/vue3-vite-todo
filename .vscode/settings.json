{
    // settings.json
    // 옵션을 추가합니다. 
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": { // 저장 버튼 누르면 자동으로 eslint의 규칙되로 변경이 됩니다.
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "html",
        "vue"
    ],
    "eslint.alwaysShowStatus": true,
    "vetur.validation.template": false
}